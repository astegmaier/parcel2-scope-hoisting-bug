{
  "name": "simple-dependency",
  "version": "1.0.0",
  "license": "MIT",
  "description": "A simple dependency that combines sideEffects: [files] with export * behavior",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "sideEffects": [
    "*.scss*",
    "lib/version.js"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "watch": "tsc -w -p ./tsconfig.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
